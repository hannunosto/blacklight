{
  "name": "mridang/blacklight",
  "description": "Blacklight helps you run code coverage for extensions built for Magento 1.x",
  "keywords": [
    "package",
    "magento"
  ],
  "homepage": "https://github.com/mridang/blacklight",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Mridang Agarwalla",
      "email": "mridang.agarwalla+blacklight@gmail.com"
    }
  ],
  "support": {
    "issues": "https://github.com/mridang/blacklight/issues"
  },
  "require": {
    "php": "^5.6 || ^7.0",
    "phpunit/php-code-blacklight": "4.0.1"
  },
  "require-dev": {
    "mridang/magazine": "0.1"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/mridang/magazine.git"
    }
  ],
  "config": {
    "vendor-dir": "lib"
  }
}
